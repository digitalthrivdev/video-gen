{
  "buildCommand": "prisma generate && prisma migrate deploy --accept-data-loss && next build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["bom1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "LOG_LEVEL": "INFO",
    "PRISMA_LOG_QUERIES": "false"
  }
}

